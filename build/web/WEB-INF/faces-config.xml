        <managed-bean-name>categoriaJpa</managed-bean-name>
        <managed-bean-class>DAO1.CategoriaFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>modelo.Categoria</converter-for-class>
        <converter-class>DAOJSF.CategoriaConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>categoria_create</from-outcome>
            <to-view-id>/CervezaJSF/categoria/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>categoria_list</from-outcome>
            <to-view-id>/CervezaJSF/categoria/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>categoria_edit</from-outcome>
            <to-view-id>/CervezaJSF/categoria/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>categoria_detail</from-outcome>
            <to-view-id>/CervezaJSF/categoria/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
